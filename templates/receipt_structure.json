{
  "template_name": "receipt",
  "template_type": "Receipt",
  "description": "Template for general receipt extraction",
  "version": "1.0.0",
  "extraction_instructions": {
    "document_type": "Receipt",
    "fields_to_extract": [
      {
        "field_name": "hospital_name",
        "description": "ชื่อโรงพยาบาล",
        "type": "string",
        "required": true,
        "examples": [
          "โรงพยาบาลกรุงเทพ",
          "โรงพยาบาลสมิติเวช"
        ]
      },
      {
        "field_name": "hn",
        "description": "หมายเลขผู้ป่วย (HN)",
        "type": "string",
        "required": true,
        "pattern": "\\d{2}-\\d{2}-\\d{6}",
        "examples": [
          "04-20-006834",
          "01-15-123456"
        ]
      },
      {
        "field_name": "an",
        "description": "หมายเลขผู้ป่วยใน (AN)",
        "type": "string",
        "required": false,
        "pattern": "\\d{3}-\\d{2}-\\d{6}",
        "examples": [
          "104-24-004742",
          "201-15-123456"
        ]
      },
      {
        "field_name": "gross_amount",
        "description": "ยอดรวมทั้งหมด",
        "type": "decimal",
        "required": true,
        "examples": [
          "445205.00",
          "32567.00"
        ]
      },
      {
        "field_name": "number_of_admit",
        "description": "จำนวนวันนอน",
        "type": "integer",
        "required": false,
        "examples": [
          "4",
          "7",
          "1"
        ]
      },
      {
        "field_name": "admission_date",
        "description": "วันที่เข้ารับการรักษา",
        "type": "date",
        "required": false,
        "format": "DD/MM/YYYY",
        "examples": [
          "2024-03-07",
          "2024-03-31"
        ]
      },
      {
        "field_name": "discharge_date",
        "description": "วันที่ออกจากโรงพยาบาล",
        "type": "date",
        "required": false,
        "format": "DD/MM/YYYY",
        "examples": [
          "2024-03-11",
          "2024-03-15"
        ]
      }
    ]
  },
